# Free Tkinter code (epi 2 )/ part 1 / adding Jpg

from tkinter import *
from PIL import Image, ImageTk

attrisha_root = Tk()

attrisha_root.geometry("1500x500")

image = Image.open("download.jpg")
photo = ImageTk.PhotoImage(image)

andrew_label = Label(image=photo)
andrew_label.pack()


attrisha_root.mainloop()
